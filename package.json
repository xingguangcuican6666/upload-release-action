{
  "name": "upload-release-action",
  "version": "3.0.3",
  "private": true,
  "description": "Upload files to a GitHub release",
  "main": "lib/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "dist": "ncc build",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn dist && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svestaro/upload-release-action.git"
  },
  "keywords": [
    "actions",
    "upload",
    "release",
    "assets",
    "asset"
  ],
  "author": "Termux Maintainers",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "@types/glob": "^7.2.0",
    "glob": "^8.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/parser": "^5.25.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.16.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.2.2",
    "jest": "^28.1.0",
    "jest-circus": "^28.1.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
