{
  "name": "upload-release-action",
  "version": "4.1.0",
  "private": true,
  "description": "Upload files to a GitHub release",
  "main": "lib/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "dist": "ncc build",
    "test": "jest",
    "all": "yarn format && yarn lint && yarn build && yarn dist && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svestaro/upload-release-action.git"
  },
  "keywords": [
    "actions",
    "upload",
    "release",
    "assets",
    "asset"
  ],
  "author": "Termux Maintainers",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@types/glob": "^8.0.0",
    "glob": "^8.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.49.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.32.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
